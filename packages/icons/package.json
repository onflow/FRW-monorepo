{
  "name": "@onflow/frw-icons",
  "version": "1.0.0",
  "description": "SVG icons package for Flow Reference Wallet with automatic React component generation",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src/components",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "generate:icons": "node scripts/icon-generator.js",
    "generate:icons:force": "node scripts/icon-generator.js --force",
    "postinstall": "npm run generate:icons",
    "clean": "rm -rf dist src/components",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "icons",
    "svg",
    "react",
    "react-native",
    "components",
    "flow",
    "wallet"
  ],
  "author": "Flow Reference Wallet Team",
  "license": "MIT",
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "eslint": "^9.17.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "19.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onflow/frw.git",
    "directory": "packages/icons"
  },
  "publishConfig": {
    "access": "public"
  }
}
