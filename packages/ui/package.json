{
  "name": "@onflow/frw-ui",
  "version": "1.0.0",
  "description": "Pure Tamagui UI components for Flow Wallet (no React Native dependencies)",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "build:watch": "tsup --watch",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest --run --passWithNoTests"
  },
  "dependencies": {
    "@onflow/frw-icons": "workspace:*",
    "@onflow/frw-types": "workspace:^",
    "@onflow/frw-utils": "workspace:^",
    "@tamagui/config": "^1.132.20",
    "@tamagui/core": "^1.132.20",
    "@tamagui/theme-builder": "^1.132.20",
    "@tamagui/web": "^1.132.20",
    "bignumber.js": "^9.1.2",
    "lottie-web": "^5.12.2",
    "tamagui": "^1.132.20"
  },
  "devDependencies": {
    "@babel/runtime": "^7.28.3",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-links": "^9.1.1",
    "@storybook/react-vite": "^9.1.1",
    "@types/node": "^22.11.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "eslint": "^9.31.0",
    "eslint-plugin-storybook": "9.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native-svg": "^15.13.0",
    "react-native-web": "^0.21.1",
    "rimraf": "^5.0.1",
    "storybook": "^9.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.4",
    "vite": "^5.4.20",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "lottie-react-native": "^6.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-native-svg": ">=15.0.0",
    "react-native-web": ">=0.21.0"
  },
  "peerDependenciesMeta": {
    "react-native-web": {
      "optional": true
    },
    "react-native": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    },
    "@tamagui/animations-react-native": {
      "optional": true
    },
    "lottie-react-native": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "src/assets"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  }
}
